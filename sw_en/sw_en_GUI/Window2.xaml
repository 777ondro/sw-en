<Window x:Class="sw_en_GUI.Window2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Tools3D="clr-namespace:_3DTools;assembly=3DTools"
    Title="Window2" Height="500" Width="700">
    <Grid x:Name="gridForTrackport">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Border Background="AliceBlue" Grid.Row="0" Grid.Column="0">
            <Tools3D:Trackport3D x:Name="_trackport" />
        </Border>
    </Grid>
</Window>
